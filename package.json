{
	"name": "create-nextplat",
	"version": "1.0.3",
	"description": "Next.js Easy-to-use Template",
	"author": "Jooy2 <jootc.help@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/jooy2/nextplat",
	"repository": {
		"type": "git",
		"url": "https://github.com/jooy2/nextplat"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"test": "npm run build && node dist/index.js",
		"build": "npm run format:fix && tsc && npm run minify",
		"minify": "terser dist/index.js --config-file .terserrc -o dist/index.js",
		"format": "prettier .",
		"format:fix": "prettier . --write"
	},
	"keywords": [
		"nextjs",
		"react",
		"template",
		"skeleton",
		"next",
		"ssr",
		"boilerplate",
		"vercel"
	],
	"devDependencies": {
		"@babel/eslint-parser": "^7.19.1",
		"@babel/plugin-syntax-import-assertions": "^7.20.0",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"eslint": "^8.24.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"prettier": "^2.7.1",
		"terser": "^5.15.1",
		"typescript": "^4.8.4"
	}
}
